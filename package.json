{
  "name": "claude-code-chat-explorer",
  "version": "1.0.0",
  "description": "Browse, search, and explore your Claude Code conversation history with a fast SQLite-powered web interface",
  "license": "MIT",
  "private": true,
  "main": "src/chats-mobile.js",
  "scripts": {
    "start": "node -e \"require('./src/chats-mobile.js').startChatsMobile()\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run test/unit",
    "test:integration": "vitest run test/integration"
  },
  "keywords": [
    "claude",
    "claude-code",
    "analytics",
    "dashboard",
    "sqlite",
    "monitoring",
    "conversations"
  ],
  "dependencies": {
    "better-sqlite3": "12.6.2",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "open": "^8.4.2",
    "qrcode": "^1.5.3",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.0",
    "supertest": "^7.0.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
